{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      selectedMap: null,\n      // Holds the currently selected map\n      // Default maps data\n      defaultMaps: [{\n        name: 'Desert Map',\n        image: '/assets/bg.png'\n      }, {\n        name: 'Forest Map',\n        image: '/assets/bg.png'\n      }, {\n        name: 'Mountain Map',\n        image: '/assets/bg.png'\n      }],\n      // Custom maps data (you can populate this dynamically later)\n      customMaps: [{\n        name: 'Custom Map 1',\n        image: '/assets/bg.png'\n      }, {\n        name: 'Custom Map 2',\n        image: '/assets/bg.png'\n      }, {\n        name: 'Custom Map 3',\n        image: '/assets/bg.png'\n      }]\n    };\n  },\n  methods: {\n    // Select a map from either default or custom maps\n    selectMap(map) {\n      this.selectedMap = map;\n    },\n    // Navigate back to the ChooseTankView\n    goBackToChooseTank() {\n      this.$router.push('/chooseTanks');\n    },\n    // Proceed to the game, passing the selected map\n    goToGame() {\n      if (this.selectedMap) {\n        this.$router.push({\n          path: '/shop',\n          query: {\n            selectedMap: this.selectedMap.name\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","selectedMap","defaultMaps","name","image","customMaps","methods","selectMap","map","goBackToChooseTank","$router","push","goToGame","path","query"],"sources":["/home/antiphy/ITU/ITU_project/src/views/ChooseMapView.vue"],"sourcesContent":["<template>\n  <div class=\"flex flex-col items-center justify-center min-h-screen\">\n    <h1 class=\"text-4xl font-bold mb-8\">Choose Map</h1>\n\n    <!-- Default Maps Section -->\n    <div class=\"grid grid-cols-3 gap-6 mb-8\">\n      <div \n        v-for=\"(map, index) in defaultMaps\" \n        :key=\"index\" \n        @click=\"selectMap(map)\"\n        :class=\"['border-4 p-4 cursor-pointer rounded-lg', selectedMap === map ? 'border-green-500' : 'border-gray-300']\"\n      >\n        <img :src=\"map.image\" :alt=\"map.name\" class=\"w-full h-40 object-cover rounded-lg\">\n        <p class=\"text-center font-semibold mt-2\">{{ map.name }}</p>\n      </div>\n    </div>\n\n    <!-- Custom Maps Section -->\n    <h2 class=\"text-2xl font-bold mb-4\">Choose Custom Map</h2>\n    <div class=\"grid grid-cols-3 gap-6 mb-8\">\n      <div \n        v-for=\"(map, index) in customMaps\" \n        :key=\"index\" \n        @click=\"selectMap(map)\"\n        :class=\"['border-4 p-4 cursor-pointer rounded-lg', selectedMap === map ? 'border-green-500' : 'border-gray-300']\"\n      >\n        <img :src=\"map.image\" :alt=\"map.name\" class=\"w-full h-40 object-cover rounded-lg\">\n        <p class=\"text-center font-semibold mt-2\">{{ map.name }}</p>\n      </div>\n    </div>\n\n    <!-- Footer Buttons -->\n    <div class=\"flex justify-between w-full px-8 mt-8\">\n      <!-- Back to Choose Tank View Button -->\n      <button @click=\"goBackToChooseTank\" class=\"px-6 py-3 bg-gray-500 text-white font-semibold rounded-lg hover:bg-gray-600 transition duration-200\">\n        Back to Choose Tank\n      </button>\n\n      <!-- Proceed to Game Button -->\n      <button @click=\"goToGame\" :disabled=\"!selectedMap\" class=\"px-6 py-3 bg-green-500 text-white font-semibold rounded-lg hover:bg-green-600 transition duration-200 disabled:bg-gray-400 disabled:cursor-not-allowed\">\n        Start Game\n      </button>\n    </div>\n  </div>\n</template>\n  \n  <script>\n  export default {\n    data() {\n      return {\n        selectedMap: null, // Holds the currently selected map\n        // Default maps data\n        defaultMaps: [\n          { name: 'Desert Map', image: '/assets/bg.png' },\n          { name: 'Forest Map', image: '/assets/bg.png' },\n          { name: 'Mountain Map', image: '/assets/bg.png' }\n        ],\n        // Custom maps data (you can populate this dynamically later)\n        customMaps: [\n          { name: 'Custom Map 1', image: '/assets/bg.png' },\n          { name: 'Custom Map 2', image: '/assets/bg.png' },\n          { name: 'Custom Map 3', image: '/assets/bg.png' }\n        ]\n      };\n    },\n    methods: {\n      // Select a map from either default or custom maps\n      selectMap(map) {\n        this.selectedMap = map;\n      },\n      // Navigate back to the ChooseTankView\n      goBackToChooseTank() {\n        this.$router.push('/chooseTanks');\n      },\n      // Proceed to the game, passing the selected map\n      goToGame() {\n        if (this.selectedMap) {\n          this.$router.push({\n            path: '/shop',\n            query: { selectedMap: this.selectedMap.name }\n          });\n        }\n      }\n    }\n  };\n  </script>\n  \n  <style scoped>\n  /* Additional styling if necessary */\n  </style>\n  "],"mappings":";AA+CE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,IAAI;MAAE;MACnB;MACAC,WAAW,EAAE,CACX;QAAEC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE;MAAiB,CAAC,EAC/C;QAAED,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE;MAAiB,CAAC,EAC/C;QAAED,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAiB,EACjD;MACD;MACAC,UAAU,EAAE,CACV;QAAEF,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAiB,CAAC,EACjD;QAAED,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAiB,CAAC,EACjD;QAAED,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAiB;IAEpD,CAAC;EACH,CAAC;EACDE,OAAO,EAAE;IACP;IACAC,SAASA,CAACC,GAAG,EAAE;MACb,IAAI,CAACP,WAAU,GAAIO,GAAG;IACxB,CAAC;IACD;IACAC,kBAAkBA,CAAA,EAAG;MACnB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,cAAc,CAAC;IACnC,CAAC;IACD;IACAC,QAAQA,CAAA,EAAG;MACT,IAAI,IAAI,CAACX,WAAW,EAAE;QACpB,IAAI,CAACS,OAAO,CAACC,IAAI,CAAC;UAChBE,IAAI,EAAE,OAAO;UACbC,KAAK,EAAE;YAAEb,WAAW,EAAE,IAAI,CAACA,WAAW,CAACE;UAAK;QAC9C,CAAC,CAAC;MACJ;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}