selectedTank;\n      }\n    },\n\n    // Update color selection for a player\n    updateColorSelection(player, selectedColor) {\n      if (player === 1) {\n        this.player1.color = selectedColor;\n      } else {\n        this.player2.color = selectedColor;\n      }\n    },\n\n    // Proceed to the map editor\n\n    ///// TODO: jeste se spravne neemitujou barvy\n    goToMapEditor() {\n      console.log('Player 1 Tank:', this.player1.tank);\n      console.log('Player 1 Color:', this.player1.color);\n      console.log('Player 2 Tank:', this.player2.tank);\n      console.log('Player 2 Color:', this.player2.color);\n    }\n  }\n}\n  </script>\n  \n  <style scoped>\n  /* You can add additional styling if needed */\n  </style>\n  "],"mappings":"AA6CE,OAAOA,YAAW,MAAO,+BAA+B;AAExD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT,CAAC;MACDC,OAAO,EAAE;QACPH,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT;IACF,CAAC;EACH,CAAC;EACDE,OAAO,EAAE;IACT;IACAC,mBAAmBA,CAACC,MAAM,EAAEC,YAAY,EAAE;MACxC,IAAID,MAAK,KAAM,CAAC,EAAE;QAChB,IAAI,CAACP,OAAO,CAACE,IAAG,GAAIM,YAAY;MAClC,OAAO;QACL,IAAI,CAACJ,OAAO,CAACF,IAAG,GAAIM,YAAY;MAClC;IACF,CAAC;IAED;IACAC,oBAAoBA,CAACF,MAAM,EAAEG,aAAa,EAAE;MAC1C,IAAIH,MAAK,KAAM,CAAC,EAAE;QAChB,IAAI,CAACP,OAAO,CAACG,KAAI,GAAIO,aAAa;MACpC,OAAO;QACL,IAAI,CAACN,OAAO,CAACD,KAAI,GAAIO,aAAa;MACpC;IACF,CAAC;IAED;;IAEA;IACAC,aAAaA,CAAA,EAAG;MACdC,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAE,IAAI,CAACb,OAAO,CAACE,IAAI,CAAC;MAChDU,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAE,IAAI,CAACb,OAAO,CAACG,KAAK,CAAC;MAClDS,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAE,IAAI,CAACT,OAAO,CAACF,IAAI,CAAC;MAChDU,OAAO,CAACC,GAAG,CAAC,iBAAiB,EAAE,IAAI,CAACT,OAAO,CAACD,KAAK,CAAC;IACpD;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}