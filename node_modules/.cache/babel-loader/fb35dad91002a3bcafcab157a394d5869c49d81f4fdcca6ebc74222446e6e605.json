{"ast":null,"code":"import { ref, computed } from 'vue';\n\n// Props\nconst maxLevel = 5;\n\n// Computed property for effectiveLevel\n\nexport default {\n  __name: 'SkillShopItem',\n  props: {\n    image: {\n      type: String,\n      default: \"\"\n    },\n    name: {\n      type: String,\n      default: \"\"\n    },\n    level: {\n      type: Number,\n      default: 0\n    },\n    availablePoints: {\n      type: Number,\n      default: 0\n    }\n  },\n  emits: ['updatePoints'],\n  setup(__props, {\n    expose: __expose,\n    emit: __emit\n  }) {\n    __expose();\n    const props = __props;\n\n    // Reactive current level, initialized from props.level\n    const currentLevel = ref(props.level);\n    const emit = __emit; // Define custom event\n\n    // Functions to upgrade/downgrade the level\n    function upgradeSkill() {\n      if (currentLevel.value < maxLevel && props.availablePoints > 0) {\n        currentLevel.value += 1;\n        emit('updatePoints', -1);\n      }\n    }\n    function downgradeSkill() {\n      if (currentLevel.value > 0) {\n        currentLevel.value -= 1;\n        emit('updatePoints', 1);\n      }\n    }\n\n    // Max level\n    const effectiveLevel = computed(() => {\n      return Math.max(0, Math.min(currentLevel.value, maxLevel));\n    });\n    const __returned__ = {\n      props,\n      currentLevel,\n      emit,\n      upgradeSkill,\n      downgradeSkill,\n      maxLevel,\n      effectiveLevel,\n      ref,\n      computed\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","maxLevel","props","__props","currentLevel","level","emit","__emit","upgradeSkill","value","availablePoints","downgradeSkill","effectiveLevel","Math","max","min"],"sources":["/home/antiphy/ITU/ITU_project/src/components/Shop/SkillShopItem.vue"],"sourcesContent":["<script setup>\nimport { defineProps, defineEmits, ref, computed } from 'vue';\n\n// Props\nconst props = defineProps({\n  image: {\n    type: String,\n    default: \"\"\n  },\n  name: {\n    type: String,\n    default: \"\"\n  },\n  level: {\n    type: Number,\n    default: 0\n  },\n\n  availablePoints:{\n    type: Number,\n    default: 0,\n  }\n});\n\n// Reactive current level, initialized from props.level\nconst currentLevel = ref(props.level);\n\nconst emit = defineEmits(['updatePoints']); // Define custom event\n\n\n// Functions to upgrade/downgrade the level\nfunction upgradeSkill() {\n  if (currentLevel.value < maxLevel && props.availablePoints > 0) {\n    currentLevel.value += 1;\n    emit('updatePoints', -1);\n  }\n}\n\nfunction downgradeSkill() {\n  if (currentLevel.value > 0) {\n    currentLevel.value -= 1;\n    emit('updatePoints', 1);\n\n  }\n}\n\n\n\n// Max level\nconst maxLevel = 5;\n\n// Computed property for effectiveLevel\nconst effectiveLevel = computed(() => {\n  return Math.max(0, Math.min(currentLevel.value, maxLevel));\n});\n</script>\n\n<template>\n  <div class=\"flex items-start\">\n    <!-- Armor Label and Icon -->\n    \n    <!-- Armor Status Bars -->\n    <div class=\"flex items-center\">\n      <img :src=\"props.image\" class=\"w-9 h-9 mr-4\" />\n\n      <!-- Active armor bars -->\n      <div\n        v-for=\"index in effectiveLevel\"\n        :key=\"index\"\n        class=\"w-12 h-12 rounded-2xl border-4 bg-yellow-300 border-yellow-400 ml-3\"\n      ></div>\n\n      <!-- Inactive armor slots -->\n      <div\n        v-for=\"index in (maxLevel - effectiveLevel)\"\n        :key=\"`inactive-${index}`\"\n        class=\"w-12 h-12 rounded-2xl border-4 bg-gray-300 border-gray-400 ml-3\"\n      >\n      </div>\n\n      <!-- Upgrade/ Downgrade Buttons -->\n      <div\n  class=\"w-12 h-12 rounded-3xl border-4 bg-green-500 border-green-600 ml-8 flex justify-center items-center cursor-pointer\n         hover:bg-green-600 hover:border-green-700 \n         active:bg-green-700 active:border-green-800\"\n  @click=\"upgradeSkill\"\n        >\n  <p class=\"text-white font-black text-2xl\">+</p>\n    </div>\n\n    <div\n      class=\"w-12 h-12 rounded-3xl border-4 bg-red-500 border-red-600 ml-2 flex justify-center items-center cursor-pointer\n         hover:bg-red-600 hover:border-red-700 \n         active:bg-red-700 active:border-red-800\"\n        @click=\"downgradeSkill\"\n    >\n        <p class=\"text-white font-black text-2xl\">-</p>\n      </div>\n\n    </div>\n  </div>\n</template>\n"],"mappings":"AACA,SAAmCA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;;AAE7D;AA8CA,MAAMC,QAAQ,GAAG,CAAC;;AAElB;;;;;;;;;;;;;;;;;;;;;;;;;;;;IA/CA,MAAMC,KAAK,GAAGC,OAkBZ;;IAEF;IACA,MAAMC,YAAY,GAAGL,GAAG,CAACG,KAAK,CAACG,KAAK,CAAC;IAErC,MAAMC,IAAI,GAAGC,MAA6B,CAAC,CAAC;;IAG5C;IACA,SAASC,YAAYA,CAAA,EAAG;MACtB,IAAIJ,YAAY,CAACK,KAAK,GAAGR,QAAQ,IAAIC,KAAK,CAACQ,eAAe,GAAG,CAAC,EAAE;QAC9DN,YAAY,CAACK,KAAK,IAAI,CAAC;QACvBH,IAAI,CAAC,cAAc,EAAE,CAAC,CAAC,CAAC;MAC1B;IACF;IAEA,SAASK,cAAcA,CAAA,EAAG;MACxB,IAAIP,YAAY,CAACK,KAAK,GAAG,CAAC,EAAE;QAC1BL,YAAY,CAACK,KAAK,IAAI,CAAC;QACvBH,IAAI,CAAC,cAAc,EAAE,CAAC,CAAC;MAEzB;IACF;;IAIA;IAIA,MAAMM,cAAc,GAAGZ,QAAQ,CAAC,MAAM;MACpC,OAAOa,IAAI,CAACC,GAAG,CAAC,CAAC,EAAED,IAAI,CAACE,GAAG,CAACX,YAAY,CAACK,KAAK,EAAER,QAAQ,CAAC,CAAC;IAC5D,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}